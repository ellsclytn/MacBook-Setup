# freshshell.com
#
# Examples:
#
#   fresh twe4ked/dotfiles shell/functions/\*
#   fresh jasoncodes/dotfiles shell/aliases/rails.sh
#   fresh jasoncodes/dotfiles config/tmux.conf --file
#
# See http://freshshell.com/readme for documentation.

fresh freshshell/fresh bin/fresh --bin
fresh freshshell/fresh contrib/source-build.sh --file=~/.zshrc

fresh iterm/com.googlecode.iterm2.plist --bin=~/Library/Preferences/com.googlecode.iterm2.plist

fresh zsh/zsh.sh
fresh-options
  fresh config/\*
  fresh env/\*
fresh-options

# Git
fresh-options --file
  fresh git/\*
fresh-options

# Vim
fresh nvim/plugs.vim --file=~/.config/nvim/init.vim
fresh-options --file=~/.config/nvim/init.vim
  fresh nvim/plugins/\*.vim
  fresh nvim/config/\*.vim
fresh-options
fresh nvim/end.vim --file=~/.config/nvim/init.vim

