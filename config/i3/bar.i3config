bar {
  position top
  status_command /usr/bin/i3status-rs ~/.config/i3status-rust/config.toml
  colors {
    background #282c34
    statusline #dddddd
    focused_workspace #e06c75 #e06c75 #c8ccd4
    active_workspace #e06c75 #e06c75 #c8ccd4
    inactive_workspace #353b45 #353b45 #282c34
    urgent_workspace #e5c07b #e5c07b #282c34
  }
}
